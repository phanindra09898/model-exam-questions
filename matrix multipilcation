#include<stdio.h>
int main()
{
    int matrix1[2][2],matrix2[2][2],i,j,mul[2][2];
    printf("Enter elements in first matrix:\n");
    for (i = 0; i < 2; i++)
    {
        for (j = 0; j < 2; j++)
        {
            scanf("%d", &matrix1[i][j]);
        }  
    }
    printf("Enter elements in second matrix:\n");
    for (i = 0; i < 2; i++)
    {
        for (j = 0; j < 2; j++)
        {
            scanf("%d", &matrix2[i][j]);
        }
    }
    for (i = 0; i < 2; i++)
    {
        for (j = 0; j < 2; j++)
        {
            mul[i][j]=matrix1[i][j]*matrix2[i][j];
        }
    }
     printf("multiplication of two matrices:\n");
      for (int i = 0; i < 2; ++i) 
      {
        for (int j = 0; j < 2; ++j) 
       {
        printf("%d   ", mul[i][j]);
       }
      printf("\n");
      }
  return 0; 
}
