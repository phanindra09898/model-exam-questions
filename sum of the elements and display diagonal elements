#include <stdio.h>

int main() {
   int matrix[3][3] = {{1, 2, 3},
                       {4, 5, 6},
                       {7, 8, 9}};
   int i, sum = 0;

   printf("The diagonal elements are: ");
   for (i = 0; i < 3; i++) {
      printf("%d ", matrix[i][i]);
      sum += matrix[i][i];
   }
   printf("\n");

   printf("The sum of the diagonal elements is: %d\n", sum);

   return 0;
}

